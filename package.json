{
    "name": "my-tick",
    "description": "gestion de ticket d'attente",
    "author": "Dev Hub",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "public/electron.js",
    "cracoConfig": "./craco.config.cjs",
    "homepage": "./",
    "dependencies": {
        "@craco/craco": "^7.1.0",
        "@nextui-org/button": "^2.0.37",
        "@nextui-org/card": "^2.0.33",
        "@nextui-org/date-picker": "^2.1.6",
        "@nextui-org/input": "^2.2.4",
        "@nextui-org/system": "^2.2.5",
        "@nextui-org/tabs": "^2.0.35",
        "@nextui-org/theme": "^2.2.9",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.8",
        "@tailwindcss/typography": "^0.5.15",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.106",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "axios": "^1.7.7",
        "cargo": "^0.8.0",
        "clsx": "^2.1.1",
        "cypress": "^13.14.1",
        "framer-motion": "^11.5.4",
        "jwt-decode": "^4.0.0",
        "lint-staged": "^15.2.10",
        "lottie-react": "^2.4.0",
        "lucide-react": "^0.438.0",
        "moment": "^2.30.1",
        "next-themes": "^0.3.0",
        "pace-js": "^1.2.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.1",
        "react-scripts": "5.0.1",
        "sass": "^1.78.0",
        "sonner": "^1.5.0",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4",
        "zustand": "^4.5.5"
    },
    "scripts": {
        "start": "concurrently \"yarn start-react\" \"wait-on http://localhost:3000 && electron public/electron.js\"",
        "start-react": "craco start",
        "build": "craco build",
        "test": "craco test",
        "test:staged": "jest --bail --findRelatedTests",
        "eject": "craco eject",
        "lint": "eslint src/**/*.{ts,tsx} --debug",
        "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
        "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,scss,md}'",
        "postinstall": "husky",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable",
        "cy:open": "cypress open",
        "package": "craco build && electron-builder",
        "electron-pack": "build --em.main=build/electron.js"
    },
    "build": {
        "appId": "com.mytick.app",
        "productName": "mytick",
        "files": [
            "build/**/*",
            "preload.js"
        ],
        "directories": {
            "output": "dist"
        }
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
    "devDependencies": {
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^8.4.0",
        "@typescript-eslint/parser": "^8.4.0",
        "autoprefixer": "^10.4.20",
        "concurrently": "^8.2.2",
        "electron": "^32.0.2",
        "electron-builder": "^25.0.5",
        "electron-is-dev": "^3.0.1",
        "electron-packager": "^17.1.2",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsx-a11y": "^6.9.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.35.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "husky": "^9.1.5",
        "postcss": "^8.4.44",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.10",
        "wait-on": "^8.0.0"
    },
    "repository": {
        "type": "bitbucket",
        "url": "git clone https://DevHubTeam@bitbucket.org/accesbanque/abm-blipp-ui.git"
    },
    "jest": {
        "moduleNameMapper": {
            "^axios$": "axios/dist/node/axios.cjs"
        },
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!coverage/**",
            "!node_modules/**",
            "!src/index.js",
            "!src/core/config/**",
            "!src/presentation/**",
            "!public/**",
            "!build/**"
        ],
        "coverageReporters": [
            "text",
            "lcov",
            "json",
            "text",
            "clover",
            "cobertura"
        ]
    }
}
